--langdef=ts
--langmap=ts:.ts
--regex-ts=/^\s*((private|public|protected|static|override|set|get|export|default|function)\s+)*(\w+)\s*(<.*>\s*)?\((\s*\)|\s*\w+\s*(\?\s*)?:)/func \3/f,function,functions/
--regex-ts=/^\s*((private|public|protected|static)\s+)*(\w+)\s*:.*;/var \3/v,variable,variables/
--regex-ts=/^(export\s+)?(let|const)\s+(\w+)/var \3/v,variable,variables/
--regex-ts=/^\s*((export|default)\s+)*(class|interface|type)\s+(\w+)\s*/\4/c,class,classes/
--regex-ts=/^\s*(declare\s+)?(module|namespace)\s+((\w|\.)+)/\2 \3/n,namespace,namespaces/
